library(plyr)
logistic <- function(x,L=1,k=1,x0=0) {
L/(1+exp(-k*(x-x0)))
}
x <- seq(-7,7,0.01)
plot(x,logistic(x),type = "l")
curves <- data.frame(x,
f=logistic(x),
f_left=logistic(x,x0=-2),
f_right=logistic(x,x0=2),
f_down=logistic(x,L=0.75),
f_down2=logistic(x,L=0.5)
)
library(ggplot2)
library(scales)
ggplot(data = curves, aes(x=x)) +
geom_line(aes(y=f)) +
geom_line(aes(y=f_left)) +
geom_line(aes(y=f_right)) +
scale_y_continuous(labels = percent) +
scale_x_continuous(labels = NULL) +
labs(title = "Competitive Antagonist", x = "log[Agonist]", y="% of max effect") +
annotate("text",x=-1.3,y=0.5,label="Control") +
annotate("text",x=0.4,y=0.5,label="I") +
annotate("text",x=2.5,y=0.5,label="3I") +
annotate("text",x=6,y=-0.02,label="Generated by Victor Chien",size=2.5)
ggplot(data = curves, aes(x=x)) +
geom_line(aes(y=f)) +
geom_line(aes(y=f_down)) +
geom_line(aes(y=f_down2)) +
scale_y_continuous(labels = percent) +
scale_x_continuous(labels = NULL) +
labs(title = "Irreversible and Non-Competitive Antagonist", x = "log[Agonist]", y="% of max effect") +
annotate("text",x=6.4,y=0.95,label="Control") +
annotate("text",x=6.5,y=0.70,label="I") +
annotate("text",x=6.5,y=0.45,label="3I") +
geom_vline(xintercept = 0) +
annotate("text",x=-1.5,y=0.95,label="No change in\nAgonist EC50") +
annotate("text",x=6,y=-0.02,label="Generated by Victor Chien",size=2.5)
